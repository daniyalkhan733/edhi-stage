---
import Base from "@/layouts/Base.astro";
import { SEO } from "astro-seo";




---

<SEO
  title="Edhi Foundation Videos - Watch and Learn"
  description="Watch Edhi Foundation's videos. Learn about our projects, impact, and the people we help through video stories."
  canonical={Astro.request.url}
  openGraph={{
    basic: {
      title: "Edhi Foundation Videos - Watch and Learn",
      type: "A type.",
      image: "https://edhiuk.org/images/logo.jpg",

    }
  }}
  twitter={{
    creator: "@jonasmerlins1"
  }}
  extend={{
    // extending the default link tags
    link: [{ rel: "icon", href: "/favicon.png" }],
    // extending the default meta tags
    meta: [
      {
        name: "twitter:image",
        content: "https://edhiuk.org/images/logo.jpg",
      },
      { name: "twitter:title", content: "Edhi Foundation Videos - Watch and Learn" },
      { name: "twitter:description", content: "Watch Edhi Foundation's videos. Learn about our projects, impact, and the people we help through video stories." },
      { name: "keywords", content: "" },
    ],
  }}
/>

<Base >
  <section><div class="px-6 py-16 mx-auto sm:max-w-xl md:max-w-full lg:max-w-screen-xl md:px-24 lg:px-8 lg:py-20">
    <div class="max-w-xl mb-10 md:mx-auto sm:text-center lg:max-w-2xl md:mb-12">
     
      <h1 class="max-w-lg mb-6 font-sans text-3xl font-bold leading-none tracking-tight text-gray-900 sm:text-4xl md:mx-auto text-center">
        
       
  Video Gallary    </h1>
      <p class="text-[17px] text-gray-700 md:text-lg font-roboto">
        Explore the compassionate work of Edhi Foundation through our captivating video gallery, showcasing moments of humanitarian aid and community support.    </p>
    </div>
    <div class="h-[300px] w-full  bg-white md:p-4 p-0">
      <swiper-container
        class="mySwiper"
        pagination="true"
        pagination-clickable="true"
        navigation="true"
        space-between="30"
        slides-per-view="3"
        loop="true"
         breakpoints='{"200": {"slidesPerView": 1, "spaceBetween": 20}, "768": {"slidesPerView": 3, "spaceBetween": 40}}'
        
      >
        <swiper-slide>
          <iframe
        class="object-cover w-full h-56 rounded shadow-lg  "
        src="https://www.youtube.com/embed/n0nKpOYpw-o?si=CK_O6tDQK_ElePjh"
        title="YouTube video player"
        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
        allowfullscreen></iframe>
        </swiper-slide>
        <swiper-slide>
          <iframe
          class="object-cover w-full h-56 rounded shadow-lg  "
          src="https://www.youtube.com/embed/QpHtJYiTKOY?si=HZJK9hy-T5bMpKGf"
          title="YouTube video player"
          allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
          allowfullscreen></iframe>      </swiper-slide>
        <swiper-slide>
           <iframe
          class="object-cover w-full h-56 rounded shadow-lg  "
          src="https://www.youtube.com/embed/gihNK_3eOSw?si=6ur3JFvZh1JBiYQ6"
          title="YouTube video player"
          allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
          allowfullscreen></iframe> 
        </swiper-slide>
        <swiper-slide>
           <iframe
          class="object-cover w-full h-56 rounded shadow-lg  "
          src="https://www.youtube.com/embed/sc0VLa2ASnA?si=Xfk4cv3ipqMkHKct"
          title="YouTube video player"
          allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
          allowfullscreen></iframe> 
        </swiper-slide>
        <swiper-slide>
           <iframe
          class="object-cover w-full h-56 rounded shadow-lg  "
          src="https://www.youtube.com/embed/oAksubepxcc?si=BnzJ-vnCqn8zwran"
          title="YouTube video player"
          allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
          allowfullscreen></iframe> 
        </swiper-slide>
      
      </swiper-container>
    </div>
    </section> 

  <style>
   
  
    swiper-container {
      width: 100%;
      height: 100%;
    }
  
    swiper-slide {
      text-align: center;
      font-size: 18px;
      background: #fff;
      display: flex;
      justify-content: center;
      align-items: center;
    }
  
    swiper-slide iframe {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
    }
    .slider {
      width: full;
      height: 350px;
    }
  </style>
  <script is:inline src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-element-bundle.min.js"
></script>


</Base>
